{
    "name": "eValue8/pricing",
    "description": "A widget that wil show pricing on your website",
    "type": "bolt-extension",
    "keywords": ["pricing", "price", "buy", "sell", "tool"],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Rakt / eValue8 BV",
            "email": "ricardo@evalue8.nl",
            "homepage": "http://evalue8.nl"
        }, {
            "name": "Yoni",
            "email": "yoni@evalue8.nl"
        }, {
            "name": "info account",
            "email": "info@evalue8.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\eValue8\\Pricing\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\eValue8\\Pricing\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-icon": "bolt-menu-editor.png",
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\eValue8\\Pricing\\PricingExtension",
        "bolt-screenshots": [
            "screenshot.png"
        ]
    }
}