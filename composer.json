{
    "name": "eValue8/pricing",
    "description": "This will show pricing on your website.",
    "type": "bolt-extension",
    "keywords": [
        "pricing",
        "price"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo",
            "email": "ricardo@evalue8.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\eValue8\\Pricing\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\eValue8\\Pricing\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\eValue8\\Pricing\\PricingExtension"
    }
}
